class Solution {
    public long maximumSubarraySum(int[] nums, int k) {
         long sum=0;
         long max=0;
         Set<Integer> set=new HashSet<>();
         int i=0;
         int j=0;
         while(j<nums.length){
            if(!set.contains(nums[j])){
                sum+=nums[j];
                set.add(nums[j]);

                if(j-i+1==k){
                    max=Math.max(sum,max);
                    sum-=nums[i];
                    set.remove(nums[i]);
                    i++;
                }
            }
            else{
                while(nums[i]!=nums[j]){
                    sum-=nums[i];
                    set.remove(nums[i]);
                    i++;
                }
                i++;
            }
            j++;
         }
         return max;
    }
}
